@model ASPFINALPROJECT.VWmodels.ViewModels


<!-- Banner Area Start -->
@foreach (var item in ViewBag.AboutBack)
{
<div class="banner-area-wrapper" style="background-image: url('Public/img/@item.Image')">

    <div class="banner-area text-center">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="banner-content-wrapper">
                        <div class="banner-content">
                            <h2>@item.PageTitle</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>}
<!-- Banner Area End -->
<!-- About Start -->
<br />
<br />
<br />
<br />

@{ Html.RenderPartial("~/Views/Shared/WtehPartial.cshtml", Model.welcomeToEduHomes); }
<!-- About End -->
<!-- Teacher Start -->
<div class="teacher-area pb-150">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="section-title text-center">
                    <img src="~/Public/img/icon/section.png" alt="title">
                    <h2>meet our teachers</h2>
                </div>
            </div>
        </div>
        <div class="row">

            @{ Html.RenderPartial("~/Views/Shared/TeacherPartial.cshtml", Model.meetOurTeachers); }
        </div>
    </div>
</div>
<!-- Teacher End -->
<!-- Testimonial Area Start -->
<div class="testimonial-area pt-110 pb-105 text-center">
    <div class="container">
        <div class="row">
            <div class="testimonial-owl owl-theme owl-carousel">


                @{ Html.RenderPartial("~/Views/Shared/StudenCom.cshtml", Model.studentSingleComments); }

            </div>
        </div>
    </div>
</div>
<!-- Testimonial Area End -->
<!-- Notice Start -->
<section class="notice-area two pt-140 pb-100">
    <div class="container">
        <div class="row">

            @{ Html.RenderPartial("~/Views/Shared/NoticePartial.cshtml", Model.noticeBoards); }
        </div>
    </div>
</section>
<!-- Notice End -->